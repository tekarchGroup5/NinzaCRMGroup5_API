{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "empId",
      "empName",
      "mobileNo",
      "email",
      "dob",
      "experience",
      "username",
      "designation",
      "password",
      "role",
      "department"
    ],
    "properties": {
      "empId": {
        "type": "string"
      },
      "empName": {
        "type": "string"
      },
      "mobileNo": {
        "type": "string",
        "pattern": "^[0-9]{10}$"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "dob": {
        "type": "string"
      },
      "experience": {
        "type": "number"
      },
      "username": {
        "type": "string"
      },
      "designation": {
        "type": ["string", "null"]
      },
      "password": {
        "type": "string"
      },
      "role": {
        "type": "string"
      },
      "department": {
        "type": ["string", "null"]
      }
    },
    "additionalProperties": false
  }
}